<?xml version="1.0" encoding="utf-8"?>
<layout>

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/frameLayout4"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f8bbd0"
        tools:context=".quiz_won_lose.QuizLose">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="378dp"
            android:layout_height="397dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="8dp"
            android:src="@drawable/lose"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/TryAgainButton"
            android:layout_width="204dp"
            android:layout_height="wrap_content"
            android:text="@string/TryAgain"
            android:textColor="#0E0A0A"
            android:textColorHighlight="#001A1515"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/LoseBackHomeButton"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView4"
            app:layout_constraintVertical_bias="0.144" />

        <Button
            android:id="@+id/LoseBackHomeButton"
            android:layout_width="204dp"
            android:layout_height="wrap_content"
            android:text="@string/GoHome"
            android:textColor="#0E0A0A"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/TryAgainButton"
            app:layout_constraintTop_toBottomOf="@+id/imageView4"
            app:layout_constraintVertical_bias="0.144" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>